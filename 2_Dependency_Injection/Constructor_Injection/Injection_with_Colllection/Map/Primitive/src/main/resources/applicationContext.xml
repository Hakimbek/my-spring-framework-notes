<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="question" class="io.spring.framework.Question">
        <constructor-arg value="11" />
        <constructor-arg value="What is Java?" />
        <constructor-arg>
            <map>
                <entry key="Java is a Programming Language" value="Hakimbek Bahramov" />
                <entry key="Java is a Platform" value="Orif" />
                <entry key="Java is an Island" value="Xurshidar" />
            </map>
        </constructor-arg>
    </bean>

</beans>